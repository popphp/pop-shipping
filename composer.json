{
    "name": "popphp/pop-shipping",
    "description": "Pop Shipping Component for Pop PHP Framework",
    "keywords": [
        "shipping api",
        "shipping rates",
        "track packages"
    ],
    "homepage": "https://www.popphp.org/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "http://www.nolainteractive.com/"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "popphp/pop-http": "^5.2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Shipping\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Shipping\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
